<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.MainMenuScreens.Rules">
    <children>
        <Label text="Rules" />
        <Button fx:id="menuButton" text="Back to Main Menu" onAction="#switchToMenu" />
        <Label text="Background - details of the background of the game"/>
        <Button text="Gameplay"/>
        <Button text="Map"/>
        <Button text="Inventory"/>
        <Button text="Events"/>
        <Button text="Towns"/>
        <Button text="Poker"/>
        <Button text="Rations"/>
        <Button text="Game Over"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
